							<opacity>1</opacity>
							<width>1</width>
							<fill>#ff0000</fill>
						</fill>
					</Marker>
					
					<Text>
						<fill>#0000ff</fill>
						<font-name>Microsoft YaHei</font-name>
						<font-size>14</font-size>
						<label-placement></label-placement>
					</Text>				
					<size>4</size>
				</PointSymbolizer>
			</Rule>		
			<Rule>			
				<MinDenominator>5000</MinDenominator>
