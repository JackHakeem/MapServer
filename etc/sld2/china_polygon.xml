
<Sld>
<!-- china map sld start -->
		<FeatureType id="china_river"> <!--江河湖海-->
			<Rule>
				<MinDenominator>50001</MinDenominator>
				<MaxDenominator>8000000</MaxDenominator> 
				<PolygonSymbolizer>					
					<Fill>
						<opacity>1</opacity>
						<width>1</width>
						<fill>#99B3CC</fill>
					</Fill>
					<Stroke>					
						<fill>#99B3CC</fill>
					</Stroke>
					<!--  双线河的文本标注显示为乱码，故取消显示文本 
					<Text>
						<fill>#000000</fill>
						<font-name>Microsoft YaHei</font-name>
						<font-size>8</font-size>
						<label-placement></label-placement>
					</Text>	
					-->
				</PolygonSymbolizer>
			</Rule>
		</FeatureType>
		<FeatureType id="china_sea"> <!--江河湖海-->
			<Rule>
				<MinDenominator>1</MinDenominator>
				<MaxDenominator>20000000.0</MaxDenominator>
				<PolygonSymbolizer>					
					<Fill>
						<opacity>1</opacity>
						<width>1</width>
						<fill>#99B3CC</fill>
					</Fill>
					<Stroke>					
						<fill>#99B3CC</fill>
					</Stroke>
					<!--  双线河的文本标注显示为乱码，故取消显示文本 
					<Text>
						<fill>#000000</fill>
						<font-name>Microsoft YaHei</font-name>
						<font-size>8</font-size>
						<label-placement></label-placement>
					</Text>	
					-->
				</PolygonSymbolizer>
			</Rule>
		</FeatureType>
		
		<FeatureType id="danxianhe"> <!-- outline -->
			<Rule>
				<MinDenominator>100001</MinDenominator>
				<MaxDenominator>2000000</MaxDenominator>	
				<LineSymbolizer>
					<Stroke>
						<opacity>1</opacity>
						<width>1</width>
						<fill>#99B3CC</fill>
						<line-join>round</line-join>
						<line-cap>round</line-cap>						
					</Stroke>															
				</LineSymbolizer>
			</Rule>
		</FeatureType>
</Sld>
