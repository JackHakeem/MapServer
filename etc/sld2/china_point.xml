
<Sld>
		<FeatureType id="china_point2">		
			<Rule>			
				<MinDenominator>1</MinDenominator>
				<MaxDenominator>500000</MaxDenominator>				 
				<PointSymbolizer>
					<Marker>
						<wellknown>square</wellknown>						
						<fill>
							<opacity>0.8</opacity>
							<width>1</width>
							<fill>#8080ff</fill>
						</fill>
					</Marker>
					
					<Text>
						<label-placement>
							<displacement>0 5</displacement>						
						</label-placement>
						<fill>#ff0000</fill>
						<font-name>Microsoft YaHei</font-name>
						<font-size>14</font-size>
						<label-placement></label-placement>
					</Text>	 			
					<size>6</size>
				</PointSymbolizer>
			</Rule>					
		</FeatureType>
