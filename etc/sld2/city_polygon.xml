
<Sld>
		<FeatureType id="back_river"> <!--江河湖海-->
			<Rule>
				<MinDenominator>1</MinDenominator>
				<MaxDenominator>150000</MaxDenominator>
				  
				<PolygonSymbolizer>					
					<Fill>
						<opacity>1</opacity>
						<width>1</width>
						<fill>#99B3CC</fill> 
					</Fill>
					<Stroke>					
						<fill>#99B3CC</fill>
					</Stroke>
					<Text>
						<fill>#000000</fill>
						<font-name>Microsoft YaHei</font-name>
						<font-size>8</font-size>
						<label-placement></label-placement>
					</Text>	
				</PolygonSymbolizer>
			</Rule>
		</FeatureType>
		  
