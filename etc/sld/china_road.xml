
<Sld>
<!-- china map sld start -->
		<FeatureType id="china_road">
			<Rule>
				<MinDenominator>200000</MinDenominator>
				<MaxDenominator>2000000</MaxDenominator>				 			
				<LineSymbolizer>
					<Stroke>
						<opacity>1</opacity>
						<width>4</width>
						<fill>#96928C</fill>
						<line-join>round</line-join>
						<line-cap>round</line-cap>						
					</Stroke>															
				</LineSymbolizer>
				<LineSymbolizer>
					<Stroke>
						<opacity>1</opacity>
						<width>2</width>
						<fill>#ffff00</fill>
						<line-join>round</line-join>
						<line-cap>round</line-cap>						
					</Stroke>	
					<Text>
						<fill>#000000</fill>
						<font-name>Microsoft YaHei</font-name>
						<font-size>8</font-size>
					</Text>									
				</LineSymbolizer>
			</Rule>			
		</FeatureType>
		<FeatureType id="china_shendao">
			<Rule>
				<MinDenominator>200000</MinDenominator>
				<MaxDenominator>2000000</MaxDenominator>				 			
				<LineSymbolizer>
					<Stroke>
						<opacity>1</opacity>
						<width>5</width>
						<fill>#96928C</fill>
						<line-join>round</line-join>
						<line-cap>round</line-cap>						
					</Stroke>															
				</LineSymbolizer>
				<LineSymbolizer>
					<Stroke>
						<opacity>1</opacity>
						<width>3</width>
						<fill>#ffff00</fill>
						<line-join>round</line-join>
						<line-cap>round</line-cap>						
					</Stroke>						
					<Text>
						<fill>#000000</fill>
						<font-name>Microsoft YaHei</font-name>
						<font-size>8</font-size>
					</Text>								
				</LineSymbolizer>
			</Rule>			
		</FeatureType>
		<FeatureType id="china_teilu">
			<Rule>
				<MinDenominator>200000</MinDenominator>
				<MaxDenominator>2000000</MaxDenominator>
				<LineSymbolizer>
					<Stroke>
						<opacity>1</opacity>
						<width>4</width>
						<fill>#96928C</fill>
						<line-join>round</line-join>
						<line-cap>round</line-cap>						
					</Stroke>															
				</LineSymbolizer>
				<LineSymbolizer>
					<Stroke>
						<opacity>1</opacity>
						<width>2</width>
						<fill>#ffffff</fill>
						<line-join>mitre</line-join>
						<line-cap>mitre</line-cap>		
						<line-dash>2 6 2 </line-dash>						
					</Stroke>															
				</LineSymbolizer>
			</Rule>			
		</FeatureType>
</Sld>